<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="3000" height="2000" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1e293b;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0f172a;stop-opacity:1" />
    </linearGradient>
    <filter id="glow">
      <feGaussianBlur stdDeviation="4" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  <rect width="100%" height="100%" fill="url(#bgGradient)" />

  <!-- AI and Human representations -->
  <g transform="translate(800, 800)">
    <!-- AI Node -->
    <circle cx="0" cy="0" r="150" fill="none" stroke="#22d3ee" stroke-width="4" stroke-dasharray="10,5" filter="url(#glow)" />
    <circle cx="0" cy="0" r="120" fill="none" stroke="#22d3ee" stroke-width="3" stroke-dasharray="5,5" />
    <circle cx="0" cy="0" r="90" fill="none" stroke="#22d3ee" stroke-width="2" stroke-dasharray="3,3" />

    <!-- Circuit pattern for AI -->
    <g stroke="#22d3ee" stroke-width="1" fill="none">
      <path d="M-80,-80 L-40,-80 L-40,-40 L40,-40 L40,40 L80,40" />
      <path d="M80,80 L40,80 L40,40 L-40,40 L-40,-40 L-80,-40" />
      <path d="M-40,80 L-40,40 L40,40 L40,-40 L80,-40" />
    </g>

    <!-- AI Icon -->
    <text x="0" y="10" text-anchor="middle" font-size="80" font-family="Arial" fill="#22d3ee" filter="url(#glow)">ðŸ¤–</text>
  </g>

  <!-- Human Node -->
  <g transform="translate(2200, 800)">
    <!-- Human Node -->
    <circle cx="0" cy="0" r="150" fill="none" stroke="#8b5cf6" stroke-width="4" stroke-dasharray="10,5" filter="url(#glow)" />
    <circle cx="0" cy="0" r="120" fill="none" stroke="#8b5cf6" stroke-width="3" stroke-dasharray="5,5" />
    <circle cx="0" cy="0" r="90" fill="none" stroke="#8b5cf6" stroke-width="2" stroke-dasharray="3,3" />

    <!-- Organic pattern for Human -->
    <g stroke="#8b5cf6" stroke-width="1" fill="none">
      <path d="M-80,0 Q-40,-80,0,0 Q40,80,80,0" />
      <path d="M0,-80 Q80,-40,0,0 Q-80,40,0,80" />
      <path d="M-60,-60 Q0,-120,60,-60 Q120,0,60,60 Q0,120,-60,60 Q-120,0,-60,-60" />
    </g>

    <!-- Human Icon -->
    <text x="0" y="10" text-anchor="middle" font-size="80" font-family="Arial" fill="#8b5cf6" filter="url(#glow)">ðŸ‘¤</text>
  </g>

  <!-- Connection between AI and Human -->
  <g stroke="#a855f7" stroke-width="2" stroke-dasharray="10,5">
    <line x1="950" y1="800" x2="2050" y2="800" stroke="#a855f7" stroke-width="4" filter="url(#glow)" />
  </g>

  <!-- Connection nodes along the line -->
  <g fill="#a855f7">
    <circle cx="1200" cy="800" r="8" fill="#a855f7" filter="url(#glow)" opacity="0.8">
      <animate attributeName="r" values="8;12;8" dur="2s" repeatCount="indefinite" />
    </circle>
    <circle cx="1400" cy="800" r="8" fill="#a855f7" filter="url(#glow)" opacity="0.8">
      <animate attributeName="r" values="8;12;8" dur="2s" repeatCount="indefinite" begin="0.5s" />
    </circle>
    <circle cx="1600" cy="800" r="8" fill="#a855f7" filter="url(#glow)" opacity="0.8">
      <animate attributeName="r" values="8;12;8" dur="2s" repeatCount="indefinite" begin="1s" />
    </circle>
    <circle cx="1800" cy="800" r="8" fill="#a855f7" filter="url(#glow)" opacity="0.8">
      <animate attributeName="r" values="8;12;8" dur="2s" repeatCount="indefinite" begin="1.5s" />
    </circle>
  </g>

  <!-- Additional abstract elements -->
  <g fill="none" stroke="#4f46e5" stroke-width="2" opacity="0.3">
    <circle cx="500" cy="500" r="200" />
    <circle cx="2500" cy="500" r="200" />
    <circle cx="1500" cy="1500" r="200" />
  </g>

  <g fill="none" stroke="#818cf8" stroke-width="1" opacity="0.2">
    <circle cx="300" cy="1700" r="150" />
    <circle cx="2700" cy="1700" r="150" />
    <circle cx="1500" cy="300" r="100" />
  </g>
</svg>